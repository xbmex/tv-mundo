<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.Casanova" name="Casanova Addons Repositorio" version="1.0.0" provider-name="Maximus">
        <extension point="xbmc.addon.repository" name="Casanova Addons Repositorio">
                <info compressed="false">https://raw.githubusercontent.com/xbmex/tv-mundo/master/addons.xml</info>
                <checksum>https://raw.githubusercontent.com/xbmex/tv-mundo/master/addons.xml.md5</checksum>
                <datadir zip="true">https://raw.githubusercontent.com/xbmex/tv-mundo/master/</datadir>
        </extension>
        <extension point="xbmc.addon.metadata">
                <summary>Maximus Addons Repositorio</summary>
                <description>Dj Casanova in The Netherlands for the next generation Latino world</description>
                <platform>all</platform>
        </extension>
</addon>

<addon id="script.module.urlresolver"
     name="URLResolver"
     version="2.9.9"
     provider-name="Eldorado">
  <requires>
    <import addon="xbmc.python" version="2.1.0" />
    <import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.simplejson" version="3.3.0"/>
    <import addon="plugin.video.youtube" version="4.4.7"/>
  </requires>
  <extension point="xbmc.python.module" library="lib" />
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Resolve common video host URL's to be playable in XBMC/Kodi.</summary>
    <description lang="en">Resolve common video host URL's to be playable in XBMC/Kodi, simplify addon development of video plugins requiring multi video hosts.</description>
  </extension>
</addon>
</addons>
